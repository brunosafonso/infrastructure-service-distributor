{
	"id": "/infrastructure-service-distributor-pg",
	"instances": 1,
	"cpus": 0.5,
	"disk": 200,
	"mem": 256,
	"networks": [
		{
			"mode": "container/bridge"
		}
	],
	"container": {
		"portMappings": [
			{
				"containerPort": 5432,
				"hostPort": 5432
			}
		],
		"type": "DOCKER",
		"volumes": [
			{
				"containerPath": "/usr/local/etc/haproxy/service",
				"mode": "RW",
				"external": {
					"name": "infrastructure-service-distributor-pg",
					"provider": "dvdi",
					"options": {
						"dvdi/driver": "rexray"
					}
				}
			}
		],
		"docker": {
			"image": "coldis/infrastructure-service-distributor-pg",
			"forcePullImage": true
		}
	},
	"env": {
		"HOSTNAME": "coldis.org",
		"INTRANET_IP": "127.0.0.1",
		"CERT_C": "BR",
		"CERT_ST": "RJ",
		"CERT_L": "Rio de Janeiro",
		"CERT_O": "Coldis",
		"CERT_OU": "Technology",
		"CERT_CN": "coldis.org",
	},
	"healthChecks": [],
	"args": [],
	"acceptedResourceRoles": [
		"slave_public"
	],
	"upgradeStrategy": {
		"minimumHealthCapacity": 0,
		"maximumOverCapacity": 0
	}
}
